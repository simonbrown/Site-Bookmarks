{
	"name": "SiteBookmarks",
	"description": "View your bookmarks on the current site from the context (right-click) menu.",
	"version": "1.5",
	"manifest_version": 2,
	"background": {
		"scripts": [ "main.js" ]
	},
	"permissions":
	[
		"bookmarks", "contextMenus"
	],
	"icons":
	{
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"options_page": "options.html",
	"minimum_chrome_version": "20.0"
}
